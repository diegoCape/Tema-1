Algoritmo Recopilacion_Datos
	
	Definir nombre Como Caracter;
	Definir apellidos Como Caracter;
	Definir edad Como Entero;
	
	Escribir "Dime tu Nombre";
	Leer nombre;
	
	Escribir "Dime tus Apellidos";
	Leer apellidos;
	
	Escribir "Dime Cuantos Años Tienes";
	Leer edad;
	
	Escribir "Tu Nombre Completo es:", " ", nombre, " ", apellidos, " ", "Y tu Edad es: ", " ", edad;
	
	
	
	
FinAlgoritmo
