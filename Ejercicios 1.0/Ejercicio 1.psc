Algoritmo Ejercicio_1
	
	Definir var_A, var_B, var_C Como Entero;
	
	var_C = 0;
	
	Escribir "Introduce el Valor para la Variable A";
	Leer var_A;
	
	Escribir "Introduce el Valor para la Variable B";
	Leer var_B;
	
	var_C = var_A; 
	var_A = var_B;
	var_B = var_C;
	
	Escribir "El Valor de la Variable A es:", " ", var_A;
	Escribir "El Valor de la Variable B es:", " ", var_B;
	
	
FinAlgoritmo
